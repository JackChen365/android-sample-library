<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <com.cz.android.sample.library.view.NestedWebView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.cz.android.sample.library.component.code.view.SourceCodeView
            android:id="@+id/sampleSourceCodeView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

    </com.cz.android.sample.library.view.NestedWebView>

    <com.cz.android.sample.library.view.WebViewProgressBar
        android:id="@+id/sampleProgressBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</FrameLayout>
